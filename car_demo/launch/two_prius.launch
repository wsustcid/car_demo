<?xml version="1.0"?>
<launch>
  <arg name="rvizconfig" default="$(find car_demo)/rviz/demo.rviz" />

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find car_demo)/worlds/mcity.world"/>
  </include>

  <node name="spawn_urdf_2" pkg="gazebo_ros" type="spawn_model" args="-file 
      $(find prius_description)/urdf/prius2.urdf -urdf -x -3 -y -12 -z 0.5 -model prius2"/>
  <!-- Begin prius 1 -->
  <node name="spawn_urdf_1" pkg="gazebo_ros" type="spawn_model" args="-file 
      $(find prius_description)/urdf/prius1.urdf -urdf -x 3 -y -12 -z 0.5 -model prius1"/>
  
  <!-- Begin prius 2 -->

 <!-- 
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" required="true" />
-->
</launch>